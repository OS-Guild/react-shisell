{
  "name": "react-shisell",
  "version": "0.0.5",
  "description": "React HoC which add analytics to your components using shisell-js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/Soluto/react-shisell",
  "author": "Soluto",
  "license": "MIT",
  "scripts": {
    "prepublish": "npm run build",
    "build": "tsc",
    "test": "jest"
  },
  "devDependencies": {
    "@types/jest": "^20.0.8",
    "@types/node": "^9.3.0",
    "@types/prop-types": "^15.5.1",
    "@types/react": "^16.0.5",
    "@types/react-test-renderer": "^15.5.4",
    "@types/recompose": "^0.24.1",
    "jest": "^21.0.1",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "ts-jest": "^21.0.0",
    "typescript": "^2.5.2"
  },
  "dependencies": {
    "recompose": "^0.25.0",
    "rxjs": "^5.4.3",
    "shisell": "^0.3.2"
  },
  "peerDependencies": {
    "react": ">=15 || ^16.0.0-0"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/__tests__/**/*.ts?(x)",
      "**/?(*.)(spec|test).ts?(x)"
    ]
  }
}
