{
    "name": "react-shisell",
    "version": "0.0.3",
    "description": "React HoC which add analytics to your components using shisell-js",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "repository": "https://github.com/Soluto/react-shisell",
    "author": "Soluto",
    "license": "MIT",
    "scripts": {
        "prepublish": "npm run build",
        "build": "tsc",
        "test": "jest",
        "precommit": "lint-staged"
    },
    "devDependencies": {
        "@types/jest": "^20.0.8",
        "@types/prop-types": "^15.5.1",
        "@types/react": "^16.0.5",
        "@types/react-test-renderer": "^15.5.4",
        "@types/recompose": "^0.24.1",
        "@types/sinon": "^2.3.3",
        "husky": "^0.14.3",
        "jest": "^21.0.1",
        "lint-staged": "^4.1.1",
        "prettier": "^1.6.1",
        "react-test-renderer": "^15.6.1",
        "sinon": "^3.2.1",
        "ts-jest": "^21.0.0",
        "typescript": "^2.5.2"
    },
    "dependencies": {
        "prop-types": "^15.5.10",
        "react": "^15.6.1",
        "recompose": "^0.25.0",
        "rxjs": "^5.4.3",
        "shisell": "^0.3.2"
    },
    "jest": {
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js"
        ],
        "transform": {
            "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
        },
        "testMatch": [
            "<rootDir>/__tests__/*.spec.(ts|tsx|js)"
        ]
    },
    "lint-staged": {
        "**/*.{js,ts,tsx}": [
            "prettier --trailing-comma es5 --single-quote --print-width 100 --tab-width 4 --bracket-spacing true --write",
            "git add"
        ]
    }
}
