{
  "name": "react-shisell",
  "version": "0.0.1",
  "description": "React HoC which add analytics to your components using shisell-js",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/Soluto/react-shisell",
  "author": "Soluto",
  "license": "MIT",
  "scripts": {
    "prepublish": "npm run build",
    "build": "tsc",
    "test": "jest"
  },
  "devDependencies": {
    "@types/react-test-renderer": "^15.5.4",
    "jest": "^21.0.1",
    "react-test-renderer": "^15.6.1",
    "typescript": "^2.5.2"
  },
  "dependencies": {
    "@types/jest": "^20.0.8",
    "@types/prop-types": "^15.5.1",
    "@types/react": "^16.0.5",
    "@types/recompose": "^0.24.1",
    "@types/sinon": "^2.3.3",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "recompose": "^0.25.0",
    "rxjs": "^5.4.3",
    "shisell": "^0.3.2",
    "sinon": "^3.2.1",
    "ts-jest": "^21.0.0"
  },
  "jest": {
    "moduleFileExtensions": ["ts", "tsx", "js"],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": ["<rootDir>/__tests__/*.spec.(ts|tsx|js)"]
  }
}
